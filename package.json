{
    "name": "theme-mitchells-midnight",
    "version": "1.0.4",
    "displayName": "Mitchell's Midnight",
    "homepage": "https://commitchell.com/",
    "description": "Fork of Dracula, with syntax colour inspiration from Dracula Soft Syntax Theme by yomed.",
    "publisher": "commitchell",
    "license": "MIT",
    "scripts": {
        "build": "node ./scripts/build.js",
        "lint": "node ./scripts/lint.js",
        "package": "vsce package -o ./bin/midnight.vsix",
        "vscode:prepublish": "npm run build",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Mitchell Simpson <hello@commitchell.com>"
    ],
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/commitchell/mitchells-midnight"
    },
    "icon": "mitchellsmidnight.png",
    "galleryBanner": {
        "color": "#181818",
        "theme": "dark"
    },
    "contributes": {
        "themes": [
            {
                "label": "Mitchell's Midnight",
                "uiTheme": "vs-dark",
                "path": "./theme/mitchells-midnight.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0",
        "prettier": "^2.6.0",
        "tinycolor2": "^1.4.2",
        "vsce": "^2.7.0",
        "minimist": ">=1.2.6"
    }
}
